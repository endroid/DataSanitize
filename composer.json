{
    "name": "endroid/data-sanitize",
    "description": "Endroid Data Sanitize",
    "keywords": ["endroid", "data", "sanitize", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/data-sanitize",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\DataSanitize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\DataSanitize\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
